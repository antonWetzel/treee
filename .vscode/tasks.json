{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "cargo",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"build",
				"--package=viewer"
			],
			"group": "build",
			"label": "cargo debug viewer"
		},
		{
			"type": "shell",
			"command": "cargo",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"build",
				"--release",
				"--package=viewer"
			],
			"group": "build",
			"label": "cargo release viewer"
		},
		{
			"type": "shell",
			"command": "cargo",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"build",
				"--package=importer"
			],
			"group": "build",
			"label": "cargo debug importer"
		},
		{
			"type": "shell",
			"command": "cargo",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"build",
				"--release",
				"--package=importer"
			],
			"group": "build",
			"label": "cargo release importer"
		},
	]
}