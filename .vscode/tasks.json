{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "cargo",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"build",
				"--package=treee"
			],
			"group": "build",
			"label": "cargo debug"
		},
		{
			"type": "shell",
			"command": "cargo",
			"problemMatcher": [
				"$rustc"
			],
			"args": [
				"build",
				"--release",
				"--package=treee"
			],
			"group": "build",
			"label": "cargo release"
		},
	]
}
